pi-container {
    width: 100%
    float: left
    .container {
        margin-right: auto
        margin-left: auto
        padding: 0
        @media (min-width: 768px) {
          width: 750px
        }

        @media (min-width: 992px) {
          width: 970px
        }

        @media (min-width: 1200px) {
          width: 1050px
        }
    }
}
