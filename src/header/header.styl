pi-header{
    width: 100%
    height: 55px
    background: #A1A1A1
    float: left
}

.nav {
    color: white
    list-style: none
    padding: 0 15px 0
}

.container {
    margin-right: auto
    margin-left: auto
    padding-left: 15px
    padding-right: 15px
    background: red
    @media (min-width: 768px) {
      width: 750px
    }

    @media (min-width: 992px) {
      width: 970px
    }

    @media (min-width: 1200px) {
      width: 1050px
    }
}
